package main

import "fmt"

func main() {
	var idade uint
	fmt.Println("Escreva a sua idade abaixo", idade)
	fmt.Scan(&idade)
	if idade <= 9 {
		fmt.Print("Você está classificado na categoria Mirim")
	} else if idade > 9 && idade <= 13 {
		fmt.Println("Você está classificado na categoria infantil")
	} else if idade >= 14 && idade <= 17 {
		fmt.Println("Você está classificado na categoria juvenil")
	} else {
		fmt.Println(" Você está classificado na categoria adulto")
	}
}
